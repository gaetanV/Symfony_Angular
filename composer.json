{
    "name": "gaetanv/symfony-javascript",
    "description": "Symfony Tools for Javascript Integration",
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/console": "^6.0",
        "doctrine/orm": "*",
        "doctrine/doctrine-bundle": "*",
        "symfony/translation": "6.0.*",
        "symfony/validator": "6.0.*",
        "symfony/form": "6.0.*",
        "symfony/yaml": "^6.0",
        "symfony/dependency-injection": "*",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SYJS\\JsBundle\\": ["src/"]
            
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SYJS\\JsBundle\\Tests\\": "tests/"
        }
    }
}
