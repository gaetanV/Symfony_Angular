<h1  ng-init=" $root.title='{% trans %}article.list{% endtrans %}' ">{% trans %}article.list{% endtrans %}</h1>

    


                
<div ng-show="loading">
    Loading
</div>
<div  ng-hide="loading">  
   
    <ul  >
        <li ng-repeat="item in list">      
              <a href="#article/{{"{{item.id}}"}}">  {{"{{item}}"}}   </a>
            <div  ng-click="remove($index,item)">X</div>
        </li>
    </ul>
</div>