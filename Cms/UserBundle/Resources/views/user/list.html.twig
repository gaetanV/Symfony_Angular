<h1  ng-init=" $root.title='{% trans %}user.list{% endtrans %}' ">{% trans %}user.list{% endtrans %}</h1>

<div ng-show="loading">
    Loading
</div>
<div  ng-hide="loading"> 
    <ul >
        <li ng-repeat="item in list">
            <a href="#user/{{"{{item.id}}"}}">{{"{{item.username}}"}}  </a>
            <div  ng-click="remove($index,item)">X</div>
        </li>
    </ul>
</div>